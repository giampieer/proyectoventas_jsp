<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <managed-bean>
        <managed-bean-name>profesor</managed-bean-name>
        <managed-bean-class>ManagerBean.ManagerProfesor</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>LOGIN</from-outcome>
            <to-view-id>/Login.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>PRINCIPALADMINISTRADOR</from-outcome>
            <to-view-id>/Vistas/Administrador/MenuPrincipalAdministrador.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>MENUPRINCIPALCLIENTE</from-outcome>
            <to-view-id>/Vistas/Cliente/MenuCliente.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>PRINCIPALCLIENTE</from-outcome>
            <to-view-id>/Vistas/Cliente/MenuPrincipalCliente.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>GRABARCLIENTE</from-outcome>
            <to-view-id>/Vistas/Administrador/MantenimientoCliente/Cliente_GrabarCliente.xhtml</to-view-id>
        </navigation-case>
          <navigation-case>
            <from-outcome>MODIFICARCLIENTE</from-outcome>
            <to-view-id>/Vistas/Administrador/MantenimientoCliente/Cliente_ModificarCliente.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>LISTADOCLIENTE</from-outcome>
            <to-view-id>/Vistas/Administrador/MantenimientoCliente/Cliente_ListarCliente.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>GRABARCURSO</from-outcome>
            <to-view-id>/Vistas/Administrador/MantenimientoCurso/Curso_GrabarCurso.xhtml</to-view-id>
        </navigation-case>
          <navigation-case>
            <from-outcome>MODIFICARCURSO</from-outcome>
            <to-view-id>/Vistas/Administrador/MantenimientoCurso/Curso_ModificarCurso.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>LISTADOCURSO</from-outcome>
            <to-view-id>/Vistas/Administrador/MantenimientoCurso/Curso_ListarCurso.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>MANADMIN</from-outcome>
            <to-view-id>/Vistas/Administrador/MantenimientoAdmin/Administrador_ListarAdministrador.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Agregarrr</from-outcome>
            <to-view-id>/Vistas/Administrador/MantenimientoAdmin/Administrador_AgregarAdministrador.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>ModificarAdmin</from-outcome>
            <to-view-id>/Vistas/Administrador/MantenimientoAdmin/Administrador_ModificarAdministrador.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Grabarfactura</from-outcome>
            <to-view-id>/Vistas/Cliente/Factura/GrabarFactura.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>ListarFactura</from-outcome>
            <to-view-id>/Vistas/Cliente/Factura/ListarFactura.xhtml</to-view-id>
        </navigation-case>
        
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>cliente</managed-bean-name>
        <managed-bean-class>ManagerBean.ManagerCliente</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>curso</managed-bean-name>
        <managed-bean-class>ManagerBean.ManagerCurso</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>administrador</managed-bean-name>
        <managed-bean-class>ManagerBean.ManagerAdministrador</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>LoginUsuario</managed-bean-name>
        <managed-bean-class>ManagerBean.ManagerLoginUsuario</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>factura</managed-bean-name>
        <managed-bean-class>ManagerBean.ManagerFactura</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
</faces-config>
